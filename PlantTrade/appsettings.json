{
    "ConnectionStrings": {
        "DefaultConnection": "Database string",
        "AzureConnectionString": "Azure string"
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "LiveReload": {
    "LiveReloadEnabled": true,
    "ClientFileExtensions": ".cshtml,.css,.js,.htm,.html,.ts,.razor,.custom",
    "ServerRefreshTimeout": 1000,
    "WebSocketUrl": "/__livereload",
    "WebSocketHost": null,
    "FolderToMonitor": "~/"
  },
  "AllowedHosts": "*"
}
  
